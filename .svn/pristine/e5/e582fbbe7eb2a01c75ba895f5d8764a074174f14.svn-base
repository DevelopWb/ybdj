<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:tools="http://schemas.android.com/tools"    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context=".actLogin" >    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_weight="1" >        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_weight="1"            android:orientation="vertical" >        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_margin="10dp"            android:orientation="vertical" >            <TableLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical" >                <TableRow                    android:id="@+id/tableRow1"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+act_login/textView1"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical"                        android:text="服务器IP:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <EditText                        android:id="@+act_login/tbIP"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:ems="10"                        android:text="192.168.1.21" />                </TableRow>                <TableRow                    android:id="@+id/TableRow01"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+act_login/TextView01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical"                        android:text="  号     码:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <EditText                        android:id="@+act_login/tbID"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:ems="10"                        android:singleLine="true"                        android:text="3" />                </TableRow>                <TableRow                    android:id="@+id/TableRow03"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical"                        android:text="  密    码:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <EditText                        android:id="@+act_login/tbPwd"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:ems="10"                        android:password="true"                        android:singleLine="true"                        android:text="3" />                </TableRow>                <TableRow                    android:id="@+id/TableRow02"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="vertical" >                        <Button                            android:id="@+act_login/btSetting"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:text="设 置" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:orientation="vertical" >                        <Button                            android:id="@+act_login/btLogin"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:text="登  录" />                    </LinearLayout>                </TableRow>            </TableLayout>        </LinearLayout>    </RelativeLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal" >        <Button            android:id="@+act_login/btRecordTest"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="录制测试" />        <Button            android:id="@+act_login/btPlayTest"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="播放测试" />        <Button            android:id="@+act_login/btRPTest"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="录播测试" />    </LinearLayout></LinearLayout>