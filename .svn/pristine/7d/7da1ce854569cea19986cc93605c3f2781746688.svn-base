<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context=".actLogin" >    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical" >        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_weight="1"            android:orientation="vertical" >            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="match_parent" >                <SurfaceView                    android:id="@+act_monitor/viCamera"                    android:layout_width="match_parent"                    android:layout_height="match_parent" />                <LinearLayout                    android:id="@+act_monitor/llMark"                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:background="#FF000000"                    android:orientation="vertical" >                </LinearLayout>                <YangTalkback.Cpt.ImageButtonEx                    android:id="@+act_monitor/ibBack"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_centerVertical="true"                    android:layout_marginRight="14dp"                    android:background="#00ffffff"                    android:src="@drawable/back" />                <TextView                    android:id="@+act_monitor/tvInfo"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerInParent="true"                    android:text="实时视频正在运行中..."                    android:textAppearance="?android:attr/textAppearanceLarge"                    android:textColor="#FF666666"                    android:textSize="30dp" />                <TextView                    android:id="@+act_monitor/tvID"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_margin="10dp"                    android:textAppearance="?android:attr/textAppearanceSmall"                    android:textColor="#99FFFFFF" />            </RelativeLayout>        </LinearLayout>    </LinearLayout></LinearLayout>