<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:tools="http://schemas.android.com/tools"    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"  >    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_weight="1"        android:background="#E3F3FF" >        <RelativeLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content" >            <ImageView                android:id="@+id/imageView1"                android:layout_width="match_parent"                android:layout_height="40dp"                android:scaleType="fitXY"                android:src="@drawable/m_top_bg" />            <TextView                android:id="@+id/textView1"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerInParent="true"                android:text="设  置"                android:textAppearance="?android:attr/textAppearanceMedium"                android:textColor="#FFFFFF" />        </RelativeLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_margin="20dp"            android:orientation="vertical" >            <TableLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:visibility="gone" >                <TableRow                    android:id="@+id/TableRow08"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:visibility="visible" >                    <TextView                        android:id="@+id/TextView08"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="分辨率:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <Spinner                            android:id="@+act_setting/ddlVideoSize"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_weight="1" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow07"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:visibility="visible" >                    <TextView                        android:id="@+id/TextView07"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="拍照模式:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <CheckBox                            android:id="@+act_setting/cbVideoCapMode"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="启用" />                        <EditText                            android:id="@+act_setting/tbPicModeQuality"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:inputType="number" />                        <TextView                            android:id="@+id/TextView05"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="质量"                            android:textAppearance="?android:attr/textAppearanceMedium" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow06"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView06"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="切换后台:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <CheckBox                            android:id="@+act_setting/cbLeaveExitApp"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="退出APP" />                        <CheckBox                            android:id="@+act_setting/cbTimeoutReconnect"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="超时重连" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow03"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="摄像头:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/radioGroup1"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbFrontCamera"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="前置" />                        <RadioButton                            android:id="@+act_setting/rbBackCamera"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="后置" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow05"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView03"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="视频采集:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/RadioGroup01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/cbVideoCaptureStopMode1"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="模式1" />                        <RadioButton                            android:id="@+act_setting/cbVideoCaptureStopMode2"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="模式2" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow04"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView02"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="视频重置间隔:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <EditText                            android:id="@+act_setting/tbVideoCaptureRestartSpan"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:inputType="number" >                            <requestFocus />                        </EditText>                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow09"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView09"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="传输方式:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/RadioGroup02"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbTCP"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="TCP" />                        <RadioButton                            android:id="@+act_setting/rbUDP"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="UDP" />                    </RadioGroup>                </TableRow>            </TableLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical" >                <TextView                    android:id="@+id/textView2"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="服务器IP设置"                    android:textAppearance="?android:attr/textAppearanceMedium"                    android:textColor="#999999" />                <EditText                    android:id="@+act_setting/tbServerIP"                    android:layout_width="match_parent"                    android:layout_height="30dp"                    android:layout_marginTop="10dp"                    android:layout_weight="1"                    android:background="@drawable/c_border1"                    android:ems="10" />            </LinearLayout>            <TableLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="20dp" >                <TableRow                    android:id="@+id/TableRow01"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView04"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="扬声器:"                        android:textAppearance="?android:attr/textAppearanceMedium"                        android:textColor="#999999" />                    <RadioGroup                        android:id="@+id/RadioGroup04"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbSpeaker1"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="听筒" />                        <RadioButton                            android:id="@+act_setting/rbSpeaker0"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="免提" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow10"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView10"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="对讲模式:"                        android:textAppearance="?android:attr/textAppearanceMedium"                        android:textColor="#999999" />                    <RadioGroup                        android:id="@+id/radioGroup2"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbSinway"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="单工" />                        <RadioButton                            android:id="@+act_setting/rbTwoway"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="双工" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow11"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView11"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="自动启动:"                        android:textAppearance="?android:attr/textAppearanceMedium"                        android:textColor="#999999" />                    <RadioGroup                        android:id="@+id/RadioGroup03"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbAutorun1"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="是    " />                        <RadioButton                            android:id="@+act_setting/rbAutorun0"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="否" />                    </RadioGroup>                </TableRow>            </TableLayout>            <Button                android:id="@+act_setting/btSave"                android:layout_width="match_parent"                android:layout_height="40dp"                android:layout_marginTop="20dp"                android:layout_weight="1"                android:background="@drawable/c_radius_button20"                android:text="保 存"                android:textColor="#ffffffff"                android:textSize="18sp" />            <Button                android:id="@+act_setting/btCancel"                android:layout_width="match_parent"                android:layout_height="40dp"                android:layout_marginTop="20dp"                android:layout_weight="1"                android:background="@drawable/c_radius_button20_1"                android:text="取 消"                android:textColor="#ffffffff"                android:textSize="18sp" />        </LinearLayout>    </RelativeLayout></LinearLayout>