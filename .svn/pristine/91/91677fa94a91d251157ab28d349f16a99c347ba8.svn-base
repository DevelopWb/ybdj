<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:tools="http://schemas.android.com/tools"    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context=".actLogin" >    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_weight="1" >        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_margin="10dp"            android:orientation="vertical" >            <TableLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical" >                <TableRow                    android:id="@+id/TableRow08"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:visibility="visible" >                    <TextView                        android:id="@+id/TextView08"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="分辨率:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <Spinner                            android:id="@+act_setting/ddlVideoSize"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_weight="1" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow07"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:visibility="visible" >                    <TextView                        android:id="@+id/TextView07"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="拍照模式:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <CheckBox                            android:id="@+act_setting/cbVideoCapMode"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="启用" />                        <EditText                            android:id="@+act_setting/tbPicModeQuality"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:inputType="number" />                        <TextView                            android:id="@+id/TextView05"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="质量"                            android:textAppearance="?android:attr/textAppearanceMedium" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow06"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView06"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="切换后台:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <CheckBox                            android:id="@+act_setting/cbLeaveExitApp"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="退出APP" />                        <CheckBox                            android:id="@+act_setting/cbTimeoutReconnect"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="超时重连" />                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow03"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="摄像头:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/radioGroup1"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbFrontCamera"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="前置" />                        <RadioButton                            android:id="@+act_setting/rbBackCamera"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="后置" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow05"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView03"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="视频采集:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/RadioGroup01"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/cbVideoCaptureStopMode1"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="模式1" />                        <RadioButton                            android:id="@+act_setting/cbVideoCaptureStopMode2"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="模式2" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow04"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView02"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="视频重置间隔:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <EditText                            android:id="@+act_setting/tbVideoCaptureRestartSpan"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:inputType="number" >                            <requestFocus />                        </EditText>                    </LinearLayout>                </TableRow>                <TableRow                    android:id="@+id/TableRow01"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView04"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="杨声器:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <CheckBox                        android:id="@+act_setting/cbSpeakMode"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:checked="true"                        android:text="免提" />                </TableRow>                <TableRow                    android:id="@+id/TableRow09"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <TextView                        android:id="@+id/TextView09"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_gravity="center_vertical|right"                        android:text="传输方式:"                        android:textAppearance="?android:attr/textAppearanceMedium" />                    <RadioGroup                        android:id="@+id/RadioGroup02"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="horizontal" >                        <RadioButton                            android:id="@+act_setting/rbTCP"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:checked="true"                            android:text="TCP" />                        <RadioButton                            android:id="@+act_setting/rbUDP"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="5dp"                            android:text="UDP" />                    </RadioGroup>                </TableRow>                <TableRow                    android:id="@+id/TableRow02"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content" >                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:orientation="vertical" >                    </LinearLayout>                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:orientation="vertical" >                        <Button                            android:id="@+act_setting/btSave"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_weight="1"                            android:text="保 存" />                    </LinearLayout>                </TableRow>            </TableLayout>        </LinearLayout>    </RelativeLayout></LinearLayout>